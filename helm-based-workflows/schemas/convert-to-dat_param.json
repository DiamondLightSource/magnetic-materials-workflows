{
  "type": "object",
  "properties": {
    "scan_numbers": {
      "type": "string",
      "default": "12345 | 12345,12346,... | 12345-12355",
      "pattern": "(^[0-9]+-[0-9]+$)|(^[0-9]+(, ?[0-9]+)*?$)"
    },
    "all_scans": {
      "type": "boolean",
      "default": false
    },
    "dat_file": {
      "type": "string",
      "default": "/spool"
    },
    "write_tiff": {
      "type": "boolean",
      "default": false
    },
    "overwrite": {
      "type": "boolean",
      "default": false    
    }
  }
}