{
  "type": "object",
  "properties": {
    "scan_numbers": {
        "type": "string",
        "default": "12345 | 12345,12346,... | 12345-12355",
        "pattern": "(^[0-9]+-[0-9]+$)|(^[0-9]+(, ?[0-9]+)*?$)"
    },
    "all_scans": {
      "type": "boolean",
      "default": true
    },
    "print_start_time": {
      "type": "boolean",
      "default": true
    },
    "print_cmd": {
      "type": "boolean",
      "default": true
    },
    "print_temp": {
      "type": "boolean",
      "default": false
    },
    "print_energy": {
      "type": "boolean",
      "default": false
    },
    "print_sx": {
      "type": "boolean",
      "default": false
    },
    "print_sy": {
      "type": "boolean",
      "default": false
    },
    "print_sz": {
      "type": "boolean",
      "default": false
    },
    "other_vars": {
      "type": "string",
      "default": "",
      "pattern": "^$|^(?:(?: ?[a-zA-Z0-9_]+ ?)+,?)+$"
    }
  }
}